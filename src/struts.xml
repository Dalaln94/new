<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	<package name="com.login" extends="struts-default">

		<action name="loginaction" class="com.login.LoginAction"
			method="execute">
			<result name="success">/success.jsp</result>
			<result name="success1">/admin.jsp</result>
			<result name="error">/loginfail.jsp</result>
		</action>
		
		<action name="register" class="com.login.RegisterAction"
			method="execute">
			<result name="success">/success.jsp</result>
			<result name="error">/loginfail.jsp</result>
		</action>
		
	
		
	
		
	
	
	

	


	</package>
	
	
</struts>